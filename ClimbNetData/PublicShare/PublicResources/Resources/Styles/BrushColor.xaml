<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:s="clr-namespace:System;assembly=mscorlib"
                    xmlns:local="clr-namespace:PublicResources.Resources.Styles">
    <!--淡蓝色-->
    <SolidColorBrush x:Key="LabelForeground1" Color="#FF4A6BD8"/>
    <!--黑色-->
    <SolidColorBrush x:Key="LabelForeground2" Color="#FF000000"/>
    <!--白色-->
    <SolidColorBrush x:Key="LabelForeground3" Color="#FFFFFFFF"/>
    <!--透明色-->
    <SolidColorBrush x:Key="Transparent" Color="#00000000"/>
    <!--灰色-->
    <SolidColorBrush x:Key="Gray" Color="#FF8D8D8D"/>
    
    <Image x:Key="InfoImage" Source="/PublicResources;component/Resources/Images/信息_128_128.png"/>
    <Image x:Key="ErrorImage" Source="/PublicResources;component/Resources/Images/错误_128_128.png"/>
    <Image x:Key="WarningImage" Source="/PublicResources;component/Resources/Images/警告_128_128.png"/>
    <Image x:Key="QuestionImage" Source="/PublicResources;component/Resources/Images/问题_128_128.png"/>
    
    <s:String x:Key="InfoTitle">信息</s:String>
    <s:String x:Key="ErrorTitle">错误</s:String>
    <s:String x:Key="WarningTitle">警告</s:String>
    <s:String x:Key="QuestionTitle">问题</s:String>

</ResourceDictionary>