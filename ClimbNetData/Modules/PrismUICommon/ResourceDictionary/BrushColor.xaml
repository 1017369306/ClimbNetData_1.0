<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:s="clr-namespace:System;assembly=mscorlib"
                    xmlns:local="clr-namespace:PrismUICommon.ResourceDictionary">
    <!--淡蓝色-->
    <SolidColorBrush x:Key="LabelForeground1" Color="#FF4A6BD8"/>
    <!--黑色-->
    <SolidColorBrush x:Key="LabelForeground2" Color="#FF000000"/>
    <!--白色-->
    <SolidColorBrush x:Key="LabelForeground3" Color="#FFFFFFFF"/>
    <!--透明色-->
    <SolidColorBrush x:Key="Transparent" Color="#00000000"/>
    <!--灰色-->
    <SolidColorBrush x:Key="Gray" Color="#FF8D8D8D"/>

    <s:String x:Key="ChooseClimbModule">ChooseClimbModule</s:String>
    <s:String x:Key="ClimbWebSite">ClimbWebSite</s:String>
    <s:String x:Key="ClimbWebSiteIndex1">ClimbWebSiteIndex1</s:String>
    <s:String x:Key="NewClimbModule">NewClimbModule</s:String>
    <s:String x:Key="SearchTextBox">SearchTextBox</s:String>
    <s:String x:Key="WebSiteTypeAndSort">WebSiteTypeAndSort</s:String>

</ResourceDictionary>